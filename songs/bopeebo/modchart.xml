<Modchart>
	<Init>
		<Shader camHUD="true" shader="BloomEffect" camGame="true" camOther="false" name="bloom2" color="#FFFFFF">
			<Property name="effect" value="0"/>
			<Property name="strength" value="0"/>
			<Property name="brightness" value="0"/>
			<Property name="contrast" value="1"/>
		</Shader>
		<Shader camHUD="false" shader="MirrorRepeatEffect" camGame="true" camOther="false" name="mirror" color="#FF0000">
			<Property name="x" value="0"/>
			<Property name="y" value="0"/>
			<Property name="angle" value="-30"/>
			<Property name="zoom" value="1"/>
			<Property name="warp" value="0"/>
			<Property name="iTime" value="0"/>
			<Property name="speed" value="1"/>
			<Property name="xRange" value="0"/>
			<Property name="yRange" value="0"/>
			<Property name="angleRange" value="0"/>
		</Shader>
		<Modifier strumLineID="1" strumID="-1" value="3" name="rotate" modifier="strumLineRotate" color="#FF00FF">
			<SubMod name="x" value="0"/>
			<SubMod name="y" value="0"/>
			<SubMod name="z" value="0"/>
		</Modifier>
		<Modifier strumLineID="-1" strumID="-1" value="2" name="drunk" modifier="drunk" color="#00FF00">
			<SubMod name="speed" value="4"/>
		</Modifier>
	</Init>
	<Events>
		<Event step="0" time="32" value="0" property="angle" ease="cubeOut" type="tweenShaderProperty" startValue="-30" name="mirror"/>
		<Event step="16" time="4" value="4" property="value" ease="cubeInOut" type="tweenModifierValue" startValue="2" name="drunk"/>
		<Event step="32" time="20" value="360" property="y" ease="cubeInOut" type="tweenModifierValue" startValue="0" name="rotate"/>
		<Event step="112" time="8" value="3" property="zoom" ease="cubeOut" type="tweenShaderProperty" startValue="1" name="mirror"/>
		<Event step="120" time="8" value="1" property="zoom" ease="cubeIn" type="tweenShaderProperty" startValue="3" name="mirror"/>
		<Event step="128" time="16" value="0" property="effect" ease="cubeOut" type="tweenShaderProperty" startValue="0.25" name="bloom2"/>
		<Event step="128" time="16" value="0" property="strength" ease="cubeOut" type="tweenShaderProperty" startValue="3" name="bloom2"/>
	</Events>
</Modchart>
